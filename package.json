{
  "name": "dashboard-rn518",
  "version": "1.0.0",
  "description": "Dashboard Financeiro RN 518 - Sistema Uniodonto para monitoramento dos 11 indicadores econômico-financeiros obrigatórios da ANS",
  "type": "module",
  "private": true,
  "author": {
    "name": "Equipe Técnica Uniodonto",
    "email": "tech@uniodonto.com.br"
  },
  "keywords": [
    "dashboard",
    "financeiro",
    "rn518",
    "ans",
    "uniodonto",
    "indicadores",
    "supabase",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/dashboard-rn518.git"
  },
  "bugs": {
    "url": "https://github.com/seu-usuario/dashboard-rn518/issues"
  },
  "homepage": "https://github.com/seu-usuario/dashboard-rn518#readme",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "verify": "npm run type-check && npm run lint && npm run format:check && npm run test:run && npm run build",
    "db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > utils/supabase/database.types.ts",
    "db:reset": "supabase db reset",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "postinstall": "npm run type-check"
  },
